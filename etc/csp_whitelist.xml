<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="sendcloud" type="host">*.sendcloud.sc</value>
                <value id="sendcloud-checkout" type="host">*.jsdelivr.net</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="sendcloud" type="host">*.sendcloud.sc</value>
                <value id="sendcloud-checkout" type="host">*.jsdelivr.net</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="sendcloud" type="host">*.sendcloud.sc</value>
                <value id="sendcloud-checkout" type="host">*.jsdelivr.net</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="sendcloud" type="host">*.sendcloud.sc</value>
                <value id="sendcloud-checkout" type="host">*.cdn.jsdelivr.net</value>
                <value id="map" type="host">*.mapbox.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="sendcloud" type="host">*.amazonaws.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
