<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="SendCloud\SendCloud\Block\Checkout\MultiShipping" name="sendcloud_shipping"
                   template="SendCloud_SendCloud::checkout/multishipping.phtml">
                <arguments>
                    <argument name="jsLayout" xsi:type="array">
                        <item name="components" xsi:type="array">
                            <item name="sendcloud-checkout" xsi:type="array">
                                <item name="component" xsi:type="string">SendCloud_SendCloud/js/multishiping/multishiping</item>
                            </item>
                        </item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
